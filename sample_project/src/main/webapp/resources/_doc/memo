CREATE TABLE "TBL_ARTICLE" 
   (	"ANO" NUMBER(5,0) NOT NULL ENABLE, 
	"TITLE" VARCHAR2(30) NOT NULL ENABLE, 
	"AUTHOR" VARCHAR2(20) NOT NULL ENABLE, 
	"READCOUNT" NUMBER DEFAULT 0 NOT NULL ENABLE, 
	"REGD8" DATE NOT NULL ENABLE, 
	"MODD8" DATE NOT NULL ENABLE, 
	"IMGFILE" VARCHAR2(100), 
	 CONSTRAINT "TBL_ARTICLE_PK" PRIMARY KEY ("ANO")


CREATE SEQUENCE SPRINGDB.SEQ_ARTICLE INCREMENT BY 1 MINVALUE 0 MAXVALUE 9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999 NOCYCLE NOCACHE NOORDER ;























